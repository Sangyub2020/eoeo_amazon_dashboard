# Supabase 설정
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Google Sheets 설정
GOOGLE_SHEETS_CLIENT_EMAIL=your_google_service_account_email
GOOGLE_SHEETS_PRIVATE_KEY=your_google_service_account_private_key

# Google Sheets ID 목록 (쉼표로 구분)
# 각 시트의 모든 탭이 자동으로 읽혀집니다
# 예: abc123def456,xyz789uvw012,ghi345jkl678
GOOGLE_SHEETS_IDS=sheet_id_1,sheet_id_2,sheet_id_3,sheet_id_4,sheet_id_5

# 제외할 탭 이름 (선택사항, 쉼표로 구분)
# 예: Summary,Template,Instructions
# 이 탭들은 동기화에서 제외됩니다
GOOGLE_SHEETS_EXCLUDE_TABS=Summary,Template,Instructions

